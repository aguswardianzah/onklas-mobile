<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/change_pass_nav"
    app:startDestination="@id/changePassForm">

    <fragment
        android:id="@+id/changePassForm"
        android:name="id.onklas.app.pages.changepass.ChangePassForm"
        android:label="ChangePassForm"
        tools:layout="@layout/change_pass_form">
        <action
            android:id="@+id/action_changePassForm_to_changePassSuccess"
            app:destination="@id/changePassSuccess"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/changePassSuccess"
        android:name="id.onklas.app.pages.changepass.ChangePassSuccess"
        android:label="ChangePassSuccess"
        tools:layout="@layout/change_pass_success" />
</navigation>